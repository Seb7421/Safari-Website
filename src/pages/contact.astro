---
import Contact from "@/components/forms/Contact.astro";
import BaseLayout from "@/layouts/BaseLayout.astro";
import type { AstroSeoProps } from "@astrolib/seo";

const seo: AstroSeoProps = {
        title: "Enquire with Mosi & Lake Safaris",
        description:
                "Share your travel dreams for Zambia or Malawi and we'll begin tailoring one of our handcrafted self-drive itineraries just for you.",
