---
import Contact from "@/components/forms/Contact.astro";
import BaseLayout from "@/layouts/BaseLayout.astro";
import type { AstroSeoProps } from "@astrolib/seo";

const seo: AstroSeoProps = {
        title: "Richiedi informazioni - Mosi & Lake Safaris",
        description:
                "Raccontaci i tuoi sogni di viaggio in Zambia o Malawi e inizieremo a modellare uno dei nostri itinerari self-drive su misura.",
        canonical: "https://mosiandlake.com/it/contact/",
};
---

<BaseLayout seo={seo}>
	<Contact />
</BaseLayout>
