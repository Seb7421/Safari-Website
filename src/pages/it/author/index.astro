---
import { getCollection } from "astro:content";
import type { AstroSeoProps } from "@astrolib/seo";
import Authors from "@components/blog/Authors.astro";
import BaseLayout from "@layouts/BaseLayout.astro";

const allAuthors = await getCollection("authors");
const pageTitle = "Tag Index";

const seo: AstroSeoProps = {
        title: "Autori - Mosi & Lake Safaris",
        description:
                "Conosci le guide e i narratori che condividono note e consigli dai nostri itinerari self-drive in Zambia e Malawi.",
        canonical: "https://mosiandlake.com/it/author/",
};
---

<BaseLayout seo={seo}>
	<Authors />
</BaseLayout>
