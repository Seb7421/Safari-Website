---
import { getCollection } from "astro:content";
import type { AstroSeoProps } from "@astrolib/seo";
import Authors from "@components/blog/Authors.astro";
import BaseLayout from "@layouts/BaseLayout.astro";

const allAuthors = await getCollection("authors");
const pageTitle = "Tag Index";

const seo: AstroSeoProps = {
        title: "Authors - Mosi & Lake Safaris",
        description:
                "Meet the guides and storytellers who share first-hand notes from our Zambia and Malawi itineraries.",
        canonical: "https://mosiandlake.com/author/",
};
---

<BaseLayout seo={seo}>
	<Authors />
</BaseLayout>
